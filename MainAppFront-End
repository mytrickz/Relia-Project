def con_test_command4(value):

    if value == "Spare":
        Label(f4, text="Temp(Spare):", width=10, anchor='w').grid(row=r+3, column=8)
        f4e9 = Entry(f4, textvariable=temp_spare4, width=6)
        f4e9.grid(row=r+3, column=9)
    elif value != "Spare":
        Label(f4, text="Temp(Spare):", width=10, anchor='w').grid(row=r+3, column=8)
        f4e6 = Label(f4, bg = "gray", width=5)
        f4e6.grid(row=r+3, column=9)
    Button(f4, text="เลือกตู้", command=confirm).grid(row=r + 3, column=10, padx=5)

