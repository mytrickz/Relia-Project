def oven_command5(value):
    if (oven5.get() == "No.1"):
        a1 = Tk()
        a1.title('ตู้ที่ 1')
        a1.geometry("500x300")
        Label(a1, text='ข้อมูลของตู้ที่ 1', width=20, anchor='w').grid(row=0, column=1)

        def dataLoad():
