def con_test_command3(value):

    if value == "Spare":
        Label(f4, text="Temp(Spare):", width=10, anchor='w').grid(row=r+2, column=8)
        f4e9 = Entry(f4, textvariable=temp_spare3, width=6)
        f4e9.grid(row=r+2, column=9)
    elif value != "Spare":
        Label(f4, text="Temp(Spare):", width=10, anchor='w').grid(row=r+2, column=8)
        f4e6 = Label(f4, bg = "gray", width=5)
        f4e6.grid(row=r+2, column=9)
    Button(f4, text="เลือกตู้", command=confirm).grid(row=r + 2, column=10, padx=5)

def type_con_command4(value):
    if (value == "HT"):
        condition = ["105°C", "125°C", "85°C", "Spare"]
        Label(f4, text="Condition test:", width=12, anchor='w').grid(row=r+3, column=6)
        f4d9 = OptionMenu(f4, cond_test4, *condition, command=con_test_command4)
        f4d9.grid(row=r+3, column=7)
    elif (value == "HM"):
        condition = ["40°C,93%RH", "85°C,85%RH", "60°C,93%RH", "Spare"]
        Label(f4, text="Condition test:", width=12, anchor='w').grid(row=r+3, column=6)
        f4d9 = OptionMenu(f4, cond_test4, *condition, command=con_test_command4)
        f4d9.grid(row=r+3, column=7)
    elif (value == "HS(A)"):
        condition = ["-55C(30min)<>125C(30min)", "-40C(30min)<>85C(30min)","Spare"]
        Label(f4, text="Condition test:", width=12, anchor='w').grid(row=r+3, column=6)
        f4d9 = OptionMenu(f4, cond_test4, *condition, command=con_test_command4)
        f4d9.grid(row=r+3, column=7)
    elif (value == "HS(L)"):
        condition = ["-"]
        Label(f4, text="Condition test:", width=12, anchor='w').grid(row=r+3, column=6)
        f4d9 = OptionMenu(f4, cond_test4, *condition, command=con_test_command4)
        f4d9.grid(row=r+3, column=7)
    elif (value == "HT+S"):
        condition = ["85°C"]
        Label(f4, text="Condition test:", width=12, anchor='w').grid(row=r+3, column=6)
        f4d9 = OptionMenu(f4, cond_test4, *condition, command=con_test_command4)
        f4d9.grid(row=r+3, column=7)
    elif (value == "PCT"):
        condition = ["Spare"]
        Label(f4, text="Condition test:", width=12, anchor='w').grid(row=r+3, column=6)
        f4d9 = OptionMenu(f4, cond_test4, *condition, command=con_test_command4)
        f4d9.grid(row=r+3, column=7)
    elif (value == "Salt spray"):
        condition = ["-"]
