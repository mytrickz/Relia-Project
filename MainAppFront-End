def type_con_command5(value):
    if (value == "HT"):
        condition = ["105°C", "125°C", "85°C", "Spare"]
        Label(f4, text="Condition test:", width=12, anchor='w').grid(row=r+4, column=6)
        f4d9 = OptionMenu(f4, cond_test5, *condition, command=con_test_command5)
        f4d9.grid(row=r+4, column=7)
    elif (value == "HM"):
        condition = ["40°C,93%RH", "85°C,85%RH", "60°C,93%RH", "Spare"]
        Label(f4, text="Condition test:", width=12, anchor='w').grid(row=r+4, column=6)
        f4d9 = OptionMenu(f4, cond_test5, *condition, command=con_test_command5)
        f4d9.grid(row=r+4, column=7)
