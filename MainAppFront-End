            Label(a1, text="-", width=20, anchor='w').grid(row=r + 12, column=2)

        Button(a1, text="Capacity Load (no. of PCB)", command=dataLoad).grid(row=r + 1, column=2)
        Label(a1, text="Type condition : HT", width=20, anchor='w').grid(row=r + 1, column=0)
        Label(a1, text="Temperature : 105°C", width=20, anchor='w').grid(row=r + 2, column=0)
        Label(a1, text="Humidity : N\A", width=20, anchor='w').grid(row=r + 3, column=0)
        Label(a1, text="Ltd : 18", width=20, anchor='w').grid(row=r + 4, column=0)
        Label(a1, text="Used : ", width=20, anchor='w').grid(row=r + 5, column=0)
        Label(a1, text="", width=20, anchor='w').grid(row=r + 5, column=1)  # ดึงData
        Label(a1, text="Balance : ", width=20, anchor='w').grid(row=r + 6, column=0)
        Label(a1, text="", width=20, anchor='w').grid(row=r + 6, column=1)  # ดึงData
    elif (oven1.get() == "No.2"):
        a1 = Tk()
