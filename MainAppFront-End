def confirm():
    print(test_cond1.get(), cond_test1.get(), temp_spare1.get())


conditionTestList = ["HT", "HM", "HS(A)", "HS(L)", "HT+S", "PCT", "Salt spray", "LT", "VT"]
f4d1 = OptionMenu(f4, test_cond1, *conditionTestList, command=type_con_command1)
f4d1.grid(row = r, column = 5)
f4d2 = OptionMenu(f4, test_cond2, *conditionTestList, command=type_con_command2)
f4d2.grid(row = r+1, column = 5)
f4d3 = OptionMenu(f4, test_cond3, *conditionTestList, command=type_con_command3)
f4d3.grid(row = r+2, column = 5)
f4d4 = OptionMenu(f4, test_cond4, *conditionTestList, command=type_con_command4)
f4d4.grid(row = r+3, column = 5)
f4d5 = OptionMenu(f4, test_cond5, *conditionTestList, command=type_con_command5)
f4d5.grid(row = r+4, column = 5)

Button(f4, text="Confirm", command=confirm).grid(row = r+6, column = 4)
Button(f4, text="เลือกตู้", command=confirm).grid(row=r, column=10, padx = 5)
Button(f4, text="เลือกตู้", command=confirm).grid(row=r+1, column=10, padx = 5)
Button(f4, text="เลือกตู้", command=confirm).grid(row=r+2, column=10, padx = 5)
Button(f4, text="เลือกตู้", command=confirm).grid(row=r+3, column=10, padx = 5)
Button(f4, text="เลือกตู้", command=confirm).grid(row=r+4, column=10, padx = 5)


root.mainloop()
