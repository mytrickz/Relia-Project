f4d3.grid(row = r+2, column = 5)
f4d4 = OptionMenu(f4, type_cond4, *conditionTestList, command=type_con_command4)
f4d4.grid(row = r+3, column = 5)
f4d5 = OptionMenu(f4, type_cond5, *conditionTestList, command=type_con_command5)
f4d5.grid(row = r+4, column = 5)

Button(f4, text="Confirm", command=confirm).grid(row = r+6, column = 4)








root.mainloop()
