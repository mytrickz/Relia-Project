def newWindow(value):
    g = Tk()
    K = value
    g.geometry("300x200")
    g.mainloop()

def confirm():
    print(type_cond1.get(), cond_test1.get(), temp_spare1.get())


def oven_command1(value):
    print(oven1)
    if (oven1.get() == "No.1"):
        a1 = Tk()
        a1.title('ตู้ที่ 1')
        a1.geometry("500x300")
        Label(a1, text='ข้อมูลของตู้ที่ 1', width=20, anchor='w').grid(row=0, column=1)

        def dataLoad():
            a1 = Tk()
            a1.title('Capacity Load ตู้ที่ 1')
            a1.geometry("500x300")
            Label(a1, text='Capacity Load', width=20, anchor='w').grid(row=1, column=1)
            Label(a1, text="Ltd", width=20, anchor='w').grid(row=1, column=2)
            Label(a1, text="Used", width=20, anchor='w').grid(row=1, column=3)
            Label(a1, text="Balance", width=20, anchor='w').grid(row=1, column=4)
            Label(a1, text="2V", width=20, anchor='w').grid(row=r + 2, column=1)
            Label(a1, text="2.2V", width=20, anchor='w').grid(row=r + 3, column=1)
            Label(a1, text="2.5V", width=20, anchor='w').grid(row=r + 4, column=1)
            Label(a1, text="4V", width=20, anchor='w').grid(row=r + 5, column=1)
            Label(a1, text="6.3V", width=20, anchor='w').grid(row=r + 6, column=1)
            Label(a1, text="10V", width=20, anchor='w').grid(row=r + 7, column=1)
            Label(a1, text="12.5V", width=20, anchor='w').grid(row=r + 8, column=1)
            Label(a1, text="16V", width=20, anchor='w').grid(row=r + 9, column=1)
            Label(a1, text="25V", width=20, anchor='w').grid(row=r + 10, column=1)
            Label(a1, text="MPT", width=20, anchor='w').grid(row=r + 11, column=1)
            Label(a1, text="Surge", width=20, anchor='w').grid(row=r + 12, column=1)
            Label(a1, text="30", width=20, anchor='w').grid(row=r + 2, column=2)
            Label(a1, text="10", width=20, anchor='w').grid(row=r + 3, column=2)
            Label(a1, text="50", width=20, anchor='w').grid(row=r + 4, column=2)
            Label(a1, text="20", width=20, anchor='w').grid(row=r + 5, column=2)
            Label(a1, text="20", width=20, anchor='w').grid(row=r + 6, column=2)
            Label(a1, text="20", width=20, anchor='w').grid(row=r + 7, column=2)
            Label(a1, text="10", width=20, anchor='w').grid(row=r + 8, column=2)
            Label(a1, text="20", width=20, anchor='w').grid(row=r + 9, column=2)
            Label(a1, text="20", width=20, anchor='w').grid(row=r + 10, column=2)
